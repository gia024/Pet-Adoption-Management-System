<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Pets_age</fullName>
    <active>true</active>
    <errorConditionFormula>OR(
    Age__c &lt;= 0,
    Age__c &gt; 12
)</errorConditionFormula>
    <errorMessage>Pet&apos;s age must be greater than 0 and less than or equal to 12 years.</errorMessage>
</ValidationRule>
